{
  "schemaVersion": 1,
  "id": "playertracer",
  "version": "${version}",
  "name": "PlayerTracer",
  "description": "Server-side only mod used to automatically record & save player data in database.",
  "authors": [
    "Subilan"
  ],
  "contact": {
    "homepage": "https://github.com/Subilan/PlayerDataTracer",
    "sources": "https://github.com/Subilan/PlayerDataTracer"
  },
  "license": "MIT",
  "icon": "assets/playertracer/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "cc.seati.fabric.PlayerTracer.MainFabric"
    ],
    "client": [
      "cc.seati.fabric.client.MainFabricClient"
    ]
  },
  "mixins": [
    "playertracer.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.15.11",
    "minecraft": "~1.20.1",
    "java": ">=17",
    "architectury": ">=9.2.14",
    "fabric-api": "*"
  },
  "suggests": {
    "another-mod": "*"
